{
    "manifest_version": 2,
    "name": "Redirecionador de URL",
    "version": "1.0",
    "permissions": ["webNavigation"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Redirecionador de URL"
    }
  }
  