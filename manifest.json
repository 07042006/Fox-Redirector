{
    "manifest_version": 2,
    "name": "Meu Redirecionador",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "webNavigation"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "web_accessible_resources": ["redirects.txt"]
  }
  